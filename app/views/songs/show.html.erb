<%= link_to "All Songs", songs_path %>
<h2><%= @song.title %></h2>
<h3><%= @song.artist_name %></h3>
<h3>Released: <%= @song.released %>, Year: <%= @song.release_year %></h3>
<h3><%= @song.genre %></h3>
<%= link_to "Edit", edit_song_path(@song) %>
<%= link_to "Destroy", song_path(@song), method: :delete %>
