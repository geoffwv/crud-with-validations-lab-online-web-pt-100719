<%= form_for(@song) do |f| %>
<div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <%= f.label :artist_name %>
    <%= f.text_field :artist_name %>
    <br>
    <%= f.label :genre %>
    <%= f.text_field :genre %>
    <br>
    <%= f.label :released %>
    <%= f.check_box :released %>
    <br>
    <%= f.label :release_year %>
    <%= f.text_field :release_year %>
  </div>

  <%= f.submit %>
<% end %>
